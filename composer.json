    "description": "An awesome Laravel Package",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "library",
    "autoload": {
        "psr-4": {
            "Yiion10\\Awesome\\": "src/"
        }
    },
    "autoload-dev": {
